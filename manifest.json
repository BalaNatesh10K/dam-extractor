{
  "manifest_version": 3,
  "name": "DAM Paths Extractor",
  "version": "1.1",
  "description": "Extracts /content/dam paths from the current page.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "512": "icon.png"
  }
}
